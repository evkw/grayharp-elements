{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "grayharp-elements": ["dist/grayharp-elements"],
      "grayharp-elements/*": ["dist/grayharp-elements/*"],
      "@example-models/*": ["projects/grayharp-examples/src/app/models/*"],
      "@example-env/*": ["projects/grayharp-examples/src/environments/*"],
      "@example-core/*": ["projects/grayharp-examples/src/app/core/*"],
    }
  }
}